<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		<title>
			{% block title %}
				Title
			{% endblock %}
		</title>
	</head>
	<body>
		<div class="container">
			{% for key,value in app.session.flashBag.get('welcome') %}
				<div class="alert alert-info" role="alert">
					{{value}}
				</div>
			{% endfor %}

			{% for key,value in app.session.flashBag.get('success') %}
				<div class="alert alert-success" role="alert">
					{{value}}
				</div>
			{% endfor %}
			{% for key,value in app.session.flashBag.get('error') %}
				<div class="alert alert-danger" role="alert">
					{{value}}
				</div>
			{% endfor %}
			{% block body %}{% endblock %}
		</div>
	</body>
	{% block javascripts %}
		{{ encore_entry_script_tags('app') }}
	{% endblock %}

</html>
